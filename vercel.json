{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "next dev",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXT_RUNTIME": "nodejs"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
